{
  "plugins": [
    "cypress"
  ],
  "env": {
    "es6": true,
    "cypress/globals": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "no-cond-assign": 2,
    "no-console": 1,
    "no-const-assign": 2,
    "no-class-assign": 2,
    "no-this-before-super": 2,
    "no-unused-vars": 1,
    "no-var": 2,
    "object-shorthand": [2, "always"]
  }
}
